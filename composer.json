{
  "name": "webman-tech/laravel-filesystem",
  "type": "library",
  "license": "MIT",
  "description": "Webman plugin webman-tech/laravel-filesystem",
  "require": {
    "php": ">=7.2",
    "illuminate/filesystem": ">=8"
  },
  "require-dev": {
    "iidestiny/flysystem-oss": "~2",
    "league/flysystem": "^1.1",
    "overtrue/flysystem-cos": "~3",
    "overtrue/flysystem-qiniu": "~1",
    "webman/console": "^1.2",
    "workerman/webman-framework": "^1.4"
  },
  "autoload": {
    "psr-4": {
      "WebmanTech\\LaravelFilesystem\\": "src"
    },
    "files": [
      "src/helper.php"
    ]
  },
  "suggest": {
    "league/flysystem:~1": "for laravel < 9",
    "league/flysystem:~3": "for laravel >= 9"
  },
  "config": {
    "sort-packages": true
  }
}
